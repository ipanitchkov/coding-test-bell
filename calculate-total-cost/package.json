{
  "name": "@panitchkov/calculate-total-cost",
  "version": "1.0.0",
  "description": "Total cost calculation based on province and discounts",
  "author": "Ivaylo Panitchkov <ivaylo.panitchkov@gmail.com>",
  "license": "UNLICENSED",
  "readmeFilename": "README.md",
  "main": "dist/test.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ipanitchkov/calculate-total-cost"
  },
  "bugs": "https://github.com/ipanitchkov/calculate-total-cost",
  "peerDependencies": {
    "@nestjs/common": "^6.0.0"
  },
  "dependencies": {
    "joi": "^17.3.0",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.6.3"
  },
  "devDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "^26.0.15",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "acorn": ">=5.7.4",
    "handlebars": "^4.7.6",
    "jest": "^26.6.1",
    "kind-of": ">=6.0.3",
    "minimist": ">=0.2.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "^26.4.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
